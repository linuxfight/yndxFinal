                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    }
                }
            }
        },
        "/login": {
            "post": {
                "description": "Войти в аккаунт с помощью логина и пароля. Возвращает JWT API Token при успешном запросе",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "Войти в аккаунт",
                "parameters": [
                    {
                        "description": "Данные для входа",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.AuthRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.AuthResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    }
                }
            }
        },
        "/register": {
            "post": {
                "description": "Создать новый аккаунт с помощью логина и пароля. Возвращает JWT API Token при успешном запросе",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "Зарегистрировать новый аккаунт",
                "parameters": [
                    {
                        "description": "Данные для регистрации",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.AuthRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/dto.AuthResponse"
                        }
                    },
                    "409": {
                        "description": "Conflict",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ApiError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.ApiError": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 500
                },
                "message": {
                    "type": "string",
                    "example": "error: nil ptr dereference"
                }
            }
        },
        "dto.AuthRequest": {
            "type": "object",
            "properties": {
                "login": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "dto.AuthResponse": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string",
                    "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJleHAiOjE3NDY2OTE0MjQsImlhdCI6MTc0NjYwNTAyNCwic3ViIjoiMDFKVEU1VE5URVZZREVQUzhHRUUwWUc0Qk0ifQ.yDYZBaEAgeAshF7zLSGNxVL8Q5P70YheQCXO68pEzHc"
                }
            }
        },
        "dto.CalculateRequest": {
            "type": "object",
            "required": [
                "expression"
            ],
            "properties": {
                "expression": {
                    "type": "string",
                    "example": "2+2"
                }
            }
        },
        "dto.CalculateResponse": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "01JTE60CDWQ5R3QSWZBP8J6FG3"
                }
            }
        },
        "dto.Expression": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "01JTE60CDWQ5R3QSWZBP8J6FG3"
                },
                "result": {
                    "type": "number",
                    "example": 4
                },
                "status": {
                    "type": "string",
                    "example": "DONE"
                }
            }
        },
        "dto.GetByIdExpressionResponse": {
            "type": "object",
            "properties": {
                "expression": {
                    "$ref": "#/definitions/dto.Expression"
                }
            }
        },
        "dto.ListAllExpressionsResponse": {
            "type": "object",
            "properties": {
                "expressions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.Expression"
                    }
                }
            }
        }
    },
    "securityDefinitions": {
        "Bearer": {
            "description": "\"Введите 'Bearer TOKEN' чтобы правильно использовать JWT API Token\"",
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}