    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/calculate": {
            "post": {
                "security": [
                    {
                        "Bearer": []
                    }
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "calculate"
                ],
                "parameters": [
                    {
                        "description": "Объект, содержащий в себе выражение",
